{
  "dashboard": {
    "id": null,
    "title": "Wazuh Security Events",
    "tags": ["wazuh", "security", "soc"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Security Events Over Time",
        "type": "graph",
        "targets": [
          {
            "expr": "wazuh_security_events_total",
            "legendFormat": "Security Events"
          }
        ],
        "yAxes": [
          {
            "unit": "short"
          }
        ]
      },
      {
        "id": 2,
        "title": "Alert Severity Distribution",
        "type": "piechart",
        "targets": [
          {
            "expr": "wazuh_alerts_by_severity",
            "legendFormat": "{{severity}}"
          }
        ]
      },
      {
        "id": 3,
        "title": "Top Attacked Hosts",
        "type": "table",
        "targets": [
          {
            "expr": "topk(10, wazuh_events_by_host)",
            "legendFormat": "{{host}}"
          }
        ]
      },
      {
        "id": 4,
        "title": "Failed Login Attempts",
        "type": "stat",
        "targets": [
          {
            "expr": "wazuh_failed_logins_total",
            "legendFormat": "Failed Logins"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "orange",
                  "value": 5
                },
                {
                  "color": "red",
                  "value": 10
                }
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "Malware Detections",
        "type": "barchart",
        "targets": [
          {
            "expr": "wazuh_malware_detections",
            "legendFormat": "{{malware_type}}"
          }
        ]
      },
      {
        "id": 6,
        "title": "Agent Status Overview",
        "type": "table",
        "targets": [
          {
            "expr": "wazuh_agent_status",
            "legendFormat": "{{agent_id}} - {{status}}"
          }
        ]
      }
    ],
    "time": {
      "from": "now-24h",
      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": []
    },
    "annotations": {
      "list": []
    },
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 0,
    "links": []
  }
}